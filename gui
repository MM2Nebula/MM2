    Title = "Start Visual Trade",
    Callback = function()
        if not visualTradeEnabled then
            WindUI:Notify({ 
                Title = "Visual Trade Error", 
                Content = "Please enable Visual Trade first!", 
                Icon = "x-circle", 
                Duration = 5 
            })
            return
        end
        
        StartVisualTrade()
    end
})

